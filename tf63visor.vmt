$rimlight "1"
$rimlightexponent 1
$rimlightboost 1
$rimmask "1"
"$selfillum" 1
Proxies
{
Sine
{
resultVar $selfillummask
"sinemin" "0"
"sinemax" "1"
"sineperiod" "1"
}
AnimatedTexture
{
"animatedTextureVar" "$basetexture"
"animatedTextureFrameNumVar" "$frame"
"animatedTextureFrameRate" "1.5"
}

}

}